{
	"name": "vue-advanced-cropper",
	"version": "2.8.8",
	"description": "The advanced library to create your own croppers suited for any website design",
	"author": "Norserium",
	"license": "MIT",
	"repository": "advanced-cropper/vue-advanced-cropper",
	"main": "dist/index.cjs.js",
	"unpkg": "dist/index.global.js",
	"jsdelivr": "dist/index.global.js",
	"module": "dist/index.esm-bundler.js",
	"types": "types/index.d.ts",
	"keywords": [
		"vue",
		"cropper",
		"library",
		"advanced",
		"lightweight",
		"customizable",
		"flex"
	],
	"engines": {
		"node": ">=8",
		"npm": ">=5"
	},
	"scripts": {
		"build": "cross-env NODE_ENV=production rollup -c && node build/build.js",
		"start": "cross-env NODE_ENV=build rollup -c -w",
		"prepare": "npm run build",
		"predeploy": "cd example && npm install && npm run build",
		"deploy": "gh-pages -d example/docs/.vuepress/dist",
		"test": "cross-env NODE_ENV=test jest",
		"lint": "eslint src/**/*.{js,vue}",
		"lint:fix": "eslint --fix src/**/*.{js,vue}"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/plugin-external-helpers": "^7.10.4",
		"@rollup/plugin-babel": "^5.0.4",
		"@rollup/plugin-commonjs": "^13.0.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"@rollup/plugin-typescript": "^6.1.0",
		"@rollup/plugin-url": "^5.0.1",
		"@types/jest": "^26.0.15",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^4.6.0",
		"@vue/babel-preset-app": "^3.12.1",
		"@vue/compiler-sfc": "~3.1.5",
		"autoprefixer": "^9.8.6",
		"babel-eslint": "^9.0.0",
		"babel-jest": "^24.9.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-stage-0": "^6.24.1",
		"cross-env": "^5.2.1",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-standard": "^11.0.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-node": "^7.0.1",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^3.1.0",
		"eslint-plugin-vue": "^5.2.3",
		"eslint-plugin-vue-libs": "^4.0.0",
		"gh-pages": "^1.2.0",
		"husky": "^3.1.0",
		"jest": "^26.6.1",
		"jest-expect-message": "^1.0.2",
		"lint-staged": "^9.5.0",
		"prettier": "^2.1.2",
		"rollup": "^2.26.10",
		"rollup-plugin-peer-deps-external": "^2.2.3",
		"rollup-plugin-scss": "^3.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-visualizer": "^1.1.1",
		"rollup-plugin-vue": "^6.0.0",
		"sass": "^1.26.10",
		"sass-loader": "^7.3.1",
		"ts-jest": "^26.4.3",
		"typescript": "^4.0.5",
		"vue": "~3.1.5"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"debounce": "^1.2.0",
		"easy-bem": "^1.0.2"
	},
	"files": [
		"dist",
		"types/index.d.ts"
	],
	"lint-staged": {
		"*.{js,vue,ts}": [
			"prettier --write",
			"eslint",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	}
}
